{
    "regions": [
        "icn1"
    ],
    "functions": {
        "pages/api/**/*": {
            "maxDuration": 60
        },
        "pages/api/*": {
            "maxDuration": 60
        }
    },
    "headers": [
        {
            "source": "/_next/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        },
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
